<%= render 'pages/home' unless user_signed_in? %>

<%= form_tag(search_pins_path, method: :get, html: {class: 'form-group'}) do %>
  <%= text_field_tag :description, params[:description], placeholder: "Enter caption search term" %>
  <%= submit_tag "Search", name: nil, class: "btn btn-success" %>
<% end %>

<%= render partial: 'pin_info' %>

  
